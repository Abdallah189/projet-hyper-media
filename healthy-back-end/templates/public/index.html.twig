    const RSS_URL = `https://abdallah189.github.io/onto/data2.rss`;
    console.log("data")

    $.ajax({
        url: RSS_URL,
        dataType: 'xml',
        success: function(data) {
            console.log(data)
            let content = ``;
            $(data).find("item").each(function () {
                let el = $(this);
                content += `<h5>${el.find("title").text()}</h5>
                            ${el.find("description").text()}`
               // console.log(el.find("title").text())
                /*document.write("------------------------");
