<script>
    $('#submitButton').click(()=>{
        name = $('#name').val()
        email = $('#email').val()
        phone = $('#phone').val()
        message = $('#message').val()
        rating = 0;
        if ($('#rating-4').is(':checked'))
            rating = 4
        if ($('#rating-5').is(':checked'))
            rating = 5
        if ($('#rating-3').is(':checked'))
            rating = 3
        if ($('#rating-2').is(':checked'))
            rating = 2
        if ($('#rating-1').is(':checked'))
            rating = 1
        console.log(rating)
        $.ajax({
            method: 'POST',
            url: '{{ path('recommendation_req') }}',
            dataType: 'json',
            data: {
                name: name,
                email: email,
                phone: phone,
                rating: rating,
                message: message
            },
            success: function (data) {
               console.log(data)
            }
        })
    })
</script>
